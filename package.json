    "react-dom": "^15.0.1",
    "react-router": "^2.3.0",
    "sw-precache": "^4.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "ava": "^0.14.0",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
