  "keywords": [],
  "repository": {
    "type": "git",
    "url": "http://github.com/winfredselwyn/react-pwa.git"
  },
  "engines": {
    "node": ">=5"
  },
  "scripts": {
    "clean": "rm assets/bundle*",
    "start": "node ./server/index.js",
    "compile": "npm run compile:webpack && gulp generate-service-worker",
    "compile:webpack": "webpack -p --progress --config ./config/webpack.production.js",
    "meta": "npm run meta:projectz",
    "meta:projectz": "projectz compile",
    "test": "nyc ava --fail-fast --verbose --require babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "babel-core": "^6.7.7",
